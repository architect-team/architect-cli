{
  "name": "architect-cli",
  "description": "Command-line interface for Architect.io",
  "version": "0.3.3",
  "author": "Architect.io",
  "bin": {
    "architect": "./bin/run"
  },
  "bugs": "https://github.com/architect-team/architect-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "auth0": "^2.20.0",
    "axios": "^0.19.0",
    "class-transformer": "^0.2.3",
    "cli-table3": "^0.5.1",
    "execa": "^3.2.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "js-yaml": "^3.13.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.10.0",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/auth0": "^2.9.22",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.4",
    "@types/fs-extra": "^8.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/js-yaml": "^3.12.1",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^5.2.7",
    "@types/moxios": "^0.4.9",
    "@types/node": "^10.17.0",
    "@types/sinon": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "eslint": "^6.5.1",
    "eslint-plugin-oclif": "^0.1.0",
    "globby": "^10.0.1",
    "listr": "^0.14.3",
    "mocha": "^5.2.0",
    "mock-spawn": "^0.2.6",
    "moxios": "^0.4.0",
    "nyc": "^13.3.0",
    "sinon": "^7.5.0",
    "ts-node": "^8.5.0",
    "typescript": "^3.7.2"
  },
  "optionalDependencies": {
    "keytar": "^4.13.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/architect-team/architect-cli",
  "keywords": [
    "oclif"
  ],
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "architect",
    "dirname": "architect",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "architect-team/architect-cli",
  "scripts": {
    "build": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "lint": "eslint --ext .ts,.js .",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
