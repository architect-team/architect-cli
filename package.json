{
  "name": "@architect-io/cli",
  "description": "Command-line interface for Architect.io",
  "version": "1.11.3-rc.38",
  "author": "Architect.io",
  "bin": {
    "architect": "./bin/run"
  },
  "bugs": "https://github.com/architect-team/architect-cli/issues",
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "3.2.2",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "ajv": "^8.6.2",
    "ajv-errors": "^3.0.0",
    "axios": "0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^3.1.0",
    "cli-table3": "0.6.0",
    "cli-ux": "5.4.9",
    "deepmerge": "^4.2.2",
    "diff": "^5.0.0",
    "execa": "5.0.0",
    "fs-extra": "9.1.0",
    "inquirer": "7.3.3",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "is-ci": "^3.0.0",
    "is-windows": "^1.0.2",
    "js-yaml": "4.1.0",
    "leven": "^3.1.0",
    "matcher": "^4.0.0",
    "opener": "^1.5.2",
    "p-limit": "^3.1.0",
    "reflect-metadata": "0.1.13",
    "semver": "^7.3.5",
    "shell-quote": "^1.7.2",
    "simple-oauth2": "^4.2.0",
    "tmp": "^0.2.1",
    "tslib": "2.1.0",
    "untildify": "4.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@oclif/test": "1.2.8",
    "@types/chai": "4.2.15",
    "@types/diff": "^5.0.1",
    "@types/fs-extra": "9.0.8",
    "@types/inquirer": "7.3.1",
    "@types/is-ci": "^2.0.0",
    "@types/is-windows": "^1.0.0",
    "@types/js-yaml": "4.0.1",
    "@types/mocha": "8.2.1",
    "@types/mock-fs": "^4.13.0",
    "@types/moxios": "0.4.10",
    "@types/node": "14.14.31",
    "@types/opener": "^1.4.0",
    "@types/semver": "^7.3.8",
    "@types/shell-quote": "^1.7.0",
    "@types/simple-oauth2": "^4.1.0",
    "@types/sinon": "9.0.10",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "chai": "4.3.0",
    "chalk": "4.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "7.32.0",
    "eslint-plugin-oclif": "0.1.0",
    "mocha": "^8.3.0",
    "mock-fs": "^4.13.0",
    "moxios": "0.4.0",
    "nock": "^13.1.3",
    "nyc": "15.1.0",
    "sinon": "9.2.4",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/architect-team/architect-cli",
  "keywords": [
    "oclif"
  ],
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "architect",
    "dirname": "architect",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "hooks": {
      "init": [
        "./lib/hooks/init/tty",
        "./lib/hooks/init/check-version"
      ]
    },
    "macos": {
      "identifier": "io.architect.cli"
    },
    "warn-if-update-available": {
      "timeoutInDays": 30
    }
  },
  "repository": "architect-team/architect-cli",
  "scripts": {
    "lint": "eslint --ext .ts,.js .",
    "fix": "eslint --ext .ts,.js . --fix",
    "build": "rm -rf ./lib && rm -f tsconfig.tsbuildinfo && tsc -b && npm run copy-static",
    "copy-static": "copyfiles -u 1 \"src/static/*.html\" lib",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "coverage": "nyc --extension .ts npm test",
    "test": "rm -rf src/dependency-manager/node_modules && mocha --forbid-only \"test/**/*.test.ts\" --config ./test/.mocharc.yml",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
